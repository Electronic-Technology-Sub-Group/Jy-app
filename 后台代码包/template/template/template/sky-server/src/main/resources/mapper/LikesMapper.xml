<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.mapper.LikesMapper">


    <select id="likesNum" resultType="java.lang.Integer">

        SELECT count(like_id)
        FROM myDemo.likes
        WHERE moment_id IN
        <foreach collection="momentIdList" item="momentId" open="(" separator="," close=")">
            #{momentId}
        </foreach>
    </select>
    



</mapper>
